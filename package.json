{
  "name": "HeySports",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "fix-xcode": "react-native-schemes-manager all",
    "newclear": "yarn run removeCachedFiles && yarn run removeCachedMetro && yarn podNewCLear && yarn install --verbose && yarn run android:clean && yarn run fix-xcode && yarn run createIconFont",
    "podInstall": "cd ios && pod install && cd ..",
    "podNewCLear": "cd ios && rm -rf Pods/* && rm -rf Podfile.lock && cd ..",
    "openXcode": "xed -b ios",
    "fix-lint": "eslint --fix src -c .eslintrc.json --ext js,jsx"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/datetimepicker": "3.4.7",
    "@react-native-community/geolocation": "2.0.2",
    "@react-native-firebase/app": "^11.5.0",
    "@react-native-firebase/auth": "^11.4.1",
    "@react-native-firebase/messaging": "^11.5.0",
    "@react-native-picker/picker": "1.15.0",
    "axios": "0.21.1",
    "intl": "1.2.5",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-date-picker": "3.2.10",
    "react-native-maps": "0.27.1",
    "react-native-navigation": "7.11.2",
    "react-native-paper": "4.8.1",
    "react-native-push-notification": "^7.3.1",
    "react-native-ratings": "7.6.1",
    "react-native-simple-radio-button": "2.7.4",
    "react-native-smooth-pincode-input": "1.0.9",
    "react-native-snap-carousel": "3.9.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-vector-icons": "8.1.0",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "seamless-immutable": "7.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "1.1.0",
    "babel-jest": "25.1.0",
    "eslint": "6.5.1",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "0.59.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
